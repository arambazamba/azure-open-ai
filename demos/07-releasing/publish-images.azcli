env=dev
grp=azure-open-ai-$env
loc=westeurope
acr=azureopenai$env
img=sk-entity-framework

az group create -n $grp -l westeurope

az acr create -g $grp -n $acr --sku Basic --admin-enabled true
az acr login -n $acr

cd $img
az acr build --image $imgApi -g $grp --registry $acr --file Dockerfile .
cd ..
